	\node(3) [draw=none, fill=none, left of=1,yshift=-1cm, xshift=-1cm]{<InlineEquation equation="\\dot{Q}" />};
	\node(4) [draw=none, fill=none, right of=1,yshift=1.5cm, xshift=1cm]{<InlineEquation equation="\\dot{W}" />};
	\node(5) [draw=none, fill=none, right of=1,yshift=-0.7cm, xshift=1.2cm]{<InlineEquation equation="\\dot{m}_{\\text{out}}" />};
